{
  "family": "chatia-backend-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::114657495916:role/ecsTaskExecutionRole-ChatIA",
  "taskRoleArn": "arn:aws:iam::114657495916:role/ecsTaskExecutionRole-ChatIA",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "chatia-backend",
      "image": "114657495916.dkr.ecr.us-east-2.amazonaws.com/chatia-backend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/chatia-backend",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
